    <!-- Contact -->
    <section id="contact">
        <h2>Contact</h2>
        <p>Your email address will not be shared. Required fields are marked with a *</p>

        <form action="">
            <fieldset id="form-name">
                <label for="name">Name<span>*</span></label>
                <input type="text" name="name" id="name" placeholder="Dave Smith" required>
            </fieldset>
            <fieldset id="form-email">
                <label for="email">Email<span>*</span></label>
                <input type="email" name="email" id="email" placeholder="dave@example.com" required>
            </fieldset>
            <fieldset id="form-no-bots">
                <label for="no-bots">Please leave this field empty</label>
                <input type="text" name="no-bots" id="no-bots" placeholder="No bots allowed">
            </fieldset>
            <fieldset id="form-message">
                <label for="message">Message*</label>
                <textarea name="message" id="message" cols="30" rows="10"
                    placeholder="Hi, how are you? Are you available to talk at 18:00 on the twelfth?"
                    required=""></textarea>
            </fieldset>
            <button type="submit">Send Message</button>
        </form>
    </section>

